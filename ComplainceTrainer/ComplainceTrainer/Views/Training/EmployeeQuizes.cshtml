@model ComplianceTrainer.Models.UserViewModel
@Html.HiddenFor(m => m.isHR, new { id = "hrCheck" })
@Html.HiddenFor(m => m.isManager, new { id = "managerCheck" })
@Html.HiddenFor(m => m.userGroups, new { id = "memberOf" })
<div hidden id="employeeId" value="@ViewBag.id">@ViewBag.id</div>
<div class="row">
    <div class="col-sm-10 col-sm-offset-1">
        <div id="spinner" style="display:block" ;>
            <h3 id="spinnerText"></h3>
            <h4>If the page takes longer then 5 seconds to load click <a onClick="window.location.reload()">Here</a></h4>
            <img src="~/Content/Images/loader32.gif" alt="loading" />
        </div>
    </div>
</div>
<div id="main-container">
    <div class="row">
        <div class="col-sm-10 col-sm-offset-1">
            <h3>Needs Grading</h3>
            <table id="needsGraded" class="display" cellspacing="0" style="width:100%;"></table>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10 col-sm-offset-1">
            <h3>Not Completed</h3>
            <table id="notCompleted" class="display" cellspacing="0" style="width:100%;"></table>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10 col-sm-offset-1">
            <h3>Completed</h3>
            <table id="completed" class="display" cellspacing="0" style="width:100%;"></table>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/employeeQuizes.js"></script>